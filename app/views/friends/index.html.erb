<div class="container">
<h1>My Friends</h1>
<% @friends.each do |friend| %>
 <div class="col-md-6">
    <h3>
    <%= image_tag friend[:picture_url] %>
    <%= link_to friend[:first_name] + " " + friend[:last_name], 
				{:controller => "goal_instances", :user_id => friend[:id]}, 
        {:class => "label label-default"} %>
    </h3>
  </div>
<% end %>

</div>
